#!/usr/bin/env php
<?php

error_reporting(E_ALL);
ini_set('display_errors', 1);



if (__DIR__ !== getcwd()) {
    if (!chdir(__DIR__)) {
        echo "ERROR: Script cannot be run from here\n";
        exit;
    }
}

require '../src/EasyShop/WebSocket/wssctl.php';
start_web_socket_server('../../');
